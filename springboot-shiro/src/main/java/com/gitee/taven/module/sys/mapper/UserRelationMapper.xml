<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.gitee.taven.module.sys.mapper.UserRelationMapper">

	<select id="getUserByUsername" resultType="com.gitee.taven.module.sys.domain.entity.User">
		select * from sys_user where `username` = #{username}
	</select>

	<resultMap id="userRolePermission" type="com.gitee.taven.module.sys.domain.dto.AuthorizationDTO">

	</resultMap>

	<select id="getUserRolePermission" resultMap="userRolePermission">

	</select>

</mapper>
